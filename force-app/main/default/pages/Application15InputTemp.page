                    );
                    // 半角数字7桁(ハイフン無し)以外の場合
                } else {

                }
            }
            
        }
        
        function doCheckDate(y, m, d) {
            y = y.substring(0, 4);
            const date = new Date(y, m - 1, d);
            return y == date.getFullYear() && m == date.getMonth()+1 && d == date.getDate();
        }
